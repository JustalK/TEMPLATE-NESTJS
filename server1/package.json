{
  "name": "project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --coverage",
    "test:load": "npx artillery run --environment load ./scenarios/scenarios.yml",
    "test:functional": "npx artillery run --environment functional ./scenarios/scenarios.yml",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc:build": "npx @compodoc/compodoc -p tsconfig.json -s",
    "type:cov": "typescript-coverage-report --threshold=90",
    "check:package": "npx npm-check -i {@src/*,@test/*,@modules/*,@shared/*}"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-express": "^3.6.7",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "colors": "^1.4.0",
    "csurf": "^1.11.0",
    "es6-shim": "^0.35.6",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^4.6.0",
    "isomorphic-fetch": "^3.0.0",
    "module-alias": "^2.2.2",
    "mongodb-memory-server": "^8.6.0",
    "mongoose": "^6.0.7",
    "nest-winston": "^1.6.2",
    "npm-check": "^5.9.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "supertest-graphql": "^1.1.4",
    "typeorm": "^0.3.6",
    "winston": "^3.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.3.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/csurf": "^1.11.2",
    "@types/es6-shim": "^0.31.42",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "artillery": "^2.0.0-19",
    "artillery-plugin-expect": "^2.0.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "loadtest": "^5.2.0",
    "mockingbird": "^2.1.2",
    "prettier": "^2.3.2",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "typescript-coverage-report": "^0.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@test(.*)$": "<rootDir>/test/$1",
      "@src(.*)$": "<rootDir>/src/$1",
      "@interfaces(.*)$": "<rootDir>/src/interfaces/$1",
      "@shared(.*)$": "<rootDir>/src/shared/$1",
      "@modules(.*)$": "<rootDir>/src/modules/$1"
    }
  },
  "_moduleAliases": {
    "@root": ".",
    "@src": "./src",
    "@test": "./test",
    "@interfaces": "./src/interfaces",
    "@shared": "./src/shared",
    "@modules": "./src/modules"
  }
}
